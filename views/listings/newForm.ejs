<% layout("/layouts/boilerplate") %>

      <body>
        
          

          

        
        <h3>Create a New Listing</h3>
    <form method="POST" action="/listings">
      <input name="title" placeholder="enter title" type="text" />
      <br /><br />
      <textarea
        name="description"
        placeholder="enter description"
      ></textarea>
      <br /><br />
      <input
        name="image"
        placeholder="enter image URL/Link"
        type="text"
      />
      <br /><br />
      <input name="price" placeholder="enter price" type="number" />
      <br /><br />
      <input name="country" placeholder="enter country" type="text" />
      <br /><br />
      <input
        name="location"
        placeholder="enter location"
        type="text"
      />
      <br /><br />
      <button>Add</button>
    </form>
      </body>
    </html>
  </body>
